{
  "manifest_version": 3,
  "name": "Abitti Exam Assessor",
  "version": "1.1",
  "description": "A browser extension to assist in assessing Abitti exam results.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://oma.abitti.fi/school/grading*"],
      "js": ["grade-ai.js"]
    }
  ],
  "icons": {
    "16": "icon.png"
  }
}
